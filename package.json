{
  "name": "quoridorn-vue-cli-3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build && npm run build-chat-log",
    "create-zip": "scripts/create-zip.sh",
    "build-chat-log": "scripts/build-chat-log.sh",
    "lint": "vue-cli-service lint",
    "set-skyway-key": "scripts/set-skyway-key.sh"
  },
  "dependencies": {
    "@types/mathjs": "^6.0.4",
    "ajv": "^6.11.0",
    "ajv-keywords": "^3.4.1",
    "compression-webpack-plugin": "^2.0.0",
    "crypto-js": "^3.3.0",
    "file-saver": "^2.0.2",
    "js-yaml": "^3.13.1",
    "json-loader": "^0.5.7",
    "jszip": "^3.2.2",
    "mathjs": "^6.6.0",
    "moment": "^2.24.0",
    "mustache": "^3.2.1",
    "python3": "0.0.1",
    "skyway-js": "^1.4.0",
    "typescript": "^3.8.2",
    "vue": "^2.6.11",
    "vue-class-component": "^6.0.0",
    "vue-mixin-decorator": "^1.2.0",
    "vue-property-decorator": "^7.3.0",
    "vue-router": "^3.1.5",
    "vue-slider-component": "^2.8.16",
    "vuedraggable": "^2.23.2",
    "vuex": "^3.1.2",
    "vuex-class": "^0.3.2",
    "yaml-loader": "^0.5.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^3.1.43",
    "@types/mustache": "^0.8.32",
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-plugin-typescript": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "@vue/eslint-config-prettier": "^4.0.0",
    "@vue/eslint-config-typescript": "^3.2.1",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.13.1",
    "sass-loader": "^7.3.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier",
      "@vue/typescript"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "typescript-eslint-parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
